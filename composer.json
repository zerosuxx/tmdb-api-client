{
    "name": "zerosuxx/tmdb-api-client",
    "description": "The Movie Database PHP Api Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tamas Mohos",
            "email": "zerosuxx@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --do-not-cache-result tests/"
    }
}
